(custom-set-variables
  ;; custom-set-variables was added by Custom.
  ;; If you edit it by hand, you could mess it up, so be careful.
  ;; Your init file should contain only one such instance.
  ;; If there is more than one, they won't work right.
 '(erc-command-indicator "--")
 '(erc-fill-column 38)
 '(erc-fill-function (quote erc-fill-variable))
 '(erc-fill-prefix "- ")
 '(erc-hide-prompt nil)
 '(erc-minibuffer-notice t)
 '(erc-prompt "#>")
 '(js2-mode-escape-quotes nil))
(custom-set-faces
  ;; custom-set-faces was added by Custom.
  ;; If you edit it by hand, you could mess it up, so be careful.
  ;; Your init file should contain only one such instance.
  ;; If there is more than one, they won't work right.
 '(erc-prompt-face ((t (:background "Black" :foreground "White" :strike-through nil :weight bold))))
 '(js2-function-name-face ((t (:foreground "chartreuse"))))
 '(js2-function-param-face ((t (:foreground "plum"))))
 '(js2-keyword-face ((t (:foreground "DarkTurquoise")))))
